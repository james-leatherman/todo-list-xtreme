{
  "id": null,
  "uid": "prometheus-combined",
  "title": "ðŸ“Š Prometheus Monitoring Dashboard",
  "description": "Comprehensive Prometheus monitoring including service health, performance metrics, and system status",
  "tags": [
    "prometheus",
    "monitoring",
    "observability",
    "infrastructure"
  ],
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "panels": [
    {
      "id": 1,
      "title": "Service Health Overview",
      "type": "stat",
      "targets": [
        {
          "expr": "up{job=\"fastapi\"}",
          "legendFormat": "FastAPI API",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        },
        {
          "expr": "up{job=\"prometheus\"}",
          "legendFormat": "Prometheus",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        },
        {
          "expr": "up{job=\"otel-collector\"}",
          "legendFormat": "OTEL Collector",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "custom": {
            "displayMode": "basic"
          },
          "mappings": [
            {
              "options": {
                "0": {
                  "text": "Down",
                  "color": "red"
                },
                "1": {
                  "text": "Up",
                  "color": "green"
                }
              },
              "type": "value"
            }
          ]
        }
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 2,
      "title": "Active Targets",
      "type": "stat",
      "targets": [
        {
          "expr": "count(up == 1)",
          "legendFormat": "Up",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        },
        {
          "expr": "count(up == 0)",
          "legendFormat": "Down",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "options": {
        "textMode": "value_and_name",
        "colorMode": "background"
      }
    },
    {
      "id": 3,
      "title": "Prometheus Query Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(prometheus_engine_queries[5m])",
          "legendFormat": "Queries/sec",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 18,
        "y": 0
      }
    },
    {
      "id": 4,
      "title": "Service Health Timeline",
      "type": "timeseries",
      "targets": [
        {
          "expr": "up",
          "legendFormat": "{{job}} - {{instance}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "stepAfter",
            "lineWidth": 2,
            "fillOpacity": 30,
            "pointSize": 5,
            "stacking": {
              "mode": "none"
            }
          },
          "min": 0,
          "max": 1,
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 6
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "current",
            "max",
            "min"
          ]
        }
      }
    },
    {
      "id": 5,
      "title": "HTTP Request Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_requests_total{job='fastapi'}[5m])",
          "legendFormat": "{{method}} {{handler}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 14
      }
    },
    {
      "id": 6,
      "title": "HTTP Response Times",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job='fastapi'}[5m]))",
          "legendFormat": "{{method}} {{handler}} (95th percentile)",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job='fastapi'}[5m]))",
          "legendFormat": "{{method}} {{handler}} (50th percentile)",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 14
      }
    },
    {
      "id": 7,
      "title": "Prometheus Internal Metrics",
      "type": "timeseries",
      "targets": [
        {
          "expr": "prometheus_tsdb_head_samples_appended_total",
          "legendFormat": "Samples Appended",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        },
        {
          "expr": "prometheus_tsdb_head_series",
          "legendFormat": "Active Series",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 22
      }
    },
    {
      "id": 8,
      "title": "Database Connections",
      "type": "timeseries",
      "targets": [
        {
          "expr": "db_connections_active",
          "legendFormat": "Active Connections",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        },
        {
          "expr": "db_connections_idle",
          "legendFormat": "Idle Connections",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 22
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h"
    ],
    "time_options": [
      "5m",
      "15m",
      "1h",
      "6h",
      "12h",
      "24h",
      "2d",
      "7d",
      "30d"
    ]
  },
  "refresh": "30s",
  "schemaVersion": 36,
  "version": 1
}