{
  "id": null,
  "title": "FastAPI Application Metrics",
  "tags": [
    "fastapi",
    "todo-list-xtreme"
  ],
  "timezone": "browser",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "HTTP Requests per Second",
      "type": "graph",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus-main",
        "name": "Prometheus"
      },
      "targets": [
        {
          "expr": "rate(http_requests_total{job='fastapi'}[5m])",
          "legendFormat": "{{method}} {{handler}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "yAxes": [
        {
          "label": "requests/sec"
        },
        {
          "show": false
        }
      ]
    },
    {
      "id": 2,
      "title": "HTTP Request Duration",
      "type": "graph",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus-main",
        "name": "Prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_highr_seconds_bucket{job='fastapi'}[5m]))",
          "legendFormat": "95th percentile",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.50, rate(http_request_duration_highr_seconds_bucket{job='fastapi'}[5m]))",
          "legendFormat": "50th percentile",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "yAxes": [
        {
          "label": "seconds"
        },
        {
          "show": false
        }
      ]
    },
    {
      "id": 3,
      "title": "HTTP Status Codes",
      "type": "graph",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus-main",
        "name": "Prometheus"
      },
      "targets": [
        {
          "expr": "rate(http_requests_total{job='fastapi'}[5m])",
          "legendFormat": "{{status}} {{method}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "title": "Active Requests",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus-main",
        "name": "Prometheus"
      },
      "targets": [
        {
          "expr": "http_requests_active{job='fastapi'}",
          "legendFormat": "Active Requests",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-main",
            "name": "Prometheus"
          }
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 12,
        "x": 12,
        "y": 8
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s",
  "uid": "fastapi-metrics"
}